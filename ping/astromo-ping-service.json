{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "astromo-ping",
    "namespace": "default",
    "labels": {
      "app": "astromo-ping"
    }
  },
  "spec": {
    "ports": [
      {
        "port": 3001,
        "targetPort": 3000,
        "nodePort": 30002
      }
    ],
    "type": "NodePort",
    "selector": {
      "app": "astromo-ping"
    }
  }
}